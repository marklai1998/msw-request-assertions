        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.mjs"
      }
    },
    "./vitest": {
      "require": {
        "types": "./dist/cjs/vitest/index.d.cts",
        "default": "./dist/cjs/vitest/index.cjs"
      },
      "import": {
        "types": "./dist/esm/vitest/index.d.ts",
        "default": "./dist/esm/vitest/index.mjs"
      }
    },
    "./jest": {
      "require": {
        "types": "./dist/cjs/jest/index.d.cts",
        "default": "./dist/cjs/jest/index.cjs"
      },
      "import": {
        "types": "./dist/esm/jest/index.d.ts",
        "default": "./dist/esm/jest/index.mjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      "vitest": [
        "./dist/esm/vitest/index.d.ts"
      ],
