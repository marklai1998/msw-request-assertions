{
  "name": "msw-request-assertions",
  "version": "1.0.5",
  "description": "Request assertions for MSW",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "default": "./dist/cjs/index.cjs"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.mjs"
      }
    },
    "./vitest": {
      "require": {
        "types": "./dist/cjs/vitest/vitest.d.cts",
        "default": "./dist/cjs/vitest/index.cjs"
      },
      "import": {
        "types": "./dist/esm/vitest/vitest.d.ts",
        "default": "./dist/esm/vitest/index.mjs"
      }
    },
    "./jest": {
      "require": {
        "types": "./dist/cjs/jest/jest.d.cts",
        "default": "./dist/cjs/jest/index.cjs"
      },
      "import": {
        "types": "./dist/esm/jest/jest.d.ts",
        "default": "./dist/esm/jest/index.mjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      "vitest": [
        "./dist/esm/vitest/index.d.ts"
      ],
      "jest": [
        "./dist/esm/jest/index.d.ts"
      ],
      "*": []
    }
  },
  "scripts": {
    "build": "rollup -c",
    "test": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marklai1998/msw-request-assertions"
  },
  "keywords": [
    "msw",
    "vitest",
    "vitest-plugin",
    "api",
    "mock",
    "mocking",
    "testing"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marklai1998/msw-request-assertion/issues"
  },
  "homepage": "https://github.com/marklai1998/msw-request-assertion#readme",
  "devDependencies": {
    "@biomejs/biome": "2.3.13",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@faker-js/faker": "10.1.0",
    "@jest/globals": "30.2.0",
    "@types/node": "24.10.1",
    "@vitest/coverage-v8": "4.0.14",
    "lefthook": "2.0.4",
    "msw": "2.12.3",
    "rollup": "4.53.3",
    "rollup-plugin-copy": "3.5.0",
    "rollup-plugin-dts": "6.3.0",
    "rollup-plugin-esbuild": "6.2.1",
    "rollup-plugin-node-externals": "8.1.2",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "typescript": "5.9.3",
    "wretch": "3.0.5"
  },
  "peerDependencies": {
    "jest": "^30.0.4",
    "msw": "^2.0.0",
    "vitest": "^3.2.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "vitest": {
      "optional": true
    },
    "jest": {
      "optional": true
    }
  },
  "packageManager": "pnpm@10.24.0",
  "engines": {
    "node": ">=18"
  }
}
